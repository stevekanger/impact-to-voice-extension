{
  "name": "Impact To Voice Extension",
  "description": "Send Impact phone numbers to google voice or text now with right click",
  "version": "0.1",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "nativeMessaging",
    "tabs"
  ],
  "background": {
    "service_worker": "scripts/serviceWorker.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": [
        "scripts/googleVoice.js"
      ],
      "matches": [
        "https://voice.google.com/*"
      ]
    },
    {
      "js": [
        "scripts/textNow.js"
      ],
      "matches": [
        "https://www.textnow.com/messaging"
      ]
    },
    {
      "js": [
        "scripts/impact.js"
      ],
      "matches": [
        "https://mobile.impact.ailife.com/Lead/*",
        "http://127.0.0.1:3000/test.html"
      ]
    }
  ],
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
